<html>
    <link rel="stylesheet" href="/header.css">

    <style>
        body {
  margin: 0px;
  overflow-x: hidden;
}
        .main{
            position: sticky;
            background-color: rgb(2, 16, 71);
            top: 0;
            height:60px;
            width: 100%;
            z-index: 2;
        }

        .bdy{
            position: relative;
            top:20%;
            left:10%;
            margin-top: -25px;
        }
        .bdy p {
            display: inline-block;
            background-color: rgb(77, 16, 105);
            border-radius: 4px;
            width:100px;
            height: 50px;
            text-align: center;
            margin-top: 5px;
            padding-top: 20px;
            margin-left: 6px;
            margin-bottom: 1px;
        }
        #corner{
            display: block;
            background-color: rgb(203, 202, 228);
            width: 100px;;
            color:rgb(8, 7, 7);
            position: relative;
            left:50px;
            top:10px;
            border-radius: 2px;
            padding-top: 5px;
            font-weight: bolder;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        .empty{
            position: absolute;
            display: block;
            left:30%;
            top:30%;
            border-radius: 3px;
            height:80px;
            width: 400px;
            border-bottom-left-radius: 10px;
            border-top-right-radius: 10px;
            border-style:ridge;
        cursor: text;
        outline: none;
        overflow-y: auto;
        }
        .empty p {
            position: relative;
            left:40px;
        }
        .display{
            display: inline-block;
            position: relative;
            background-color: rgb(226, 221, 221);
            width: 60%;
            height: 88%;
            top:10px;
            overflow: auto;
            border-radius: 4px;
        }
        .mainbtn{
           /* background-color: rgb(35, 11, 100); */
           background-color: rgba(53, 33, 109, 0.589);
            color: black;
            border-radius: 4px;
            border-top: 4px;
            border-bottom-style: dashed;
            margin: 6px;
            max-width: 200px;
            word-wrap: break-word;
            height:50px;
            overflow-y: hidden;
            cursor: pointer;
            font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;

        }
      

       
        .display p{
          position: relative;
          left:300px;
          top:140px;
      }
      

    </style>
    <body>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>

$(document).ready(()=>{
    $("#mainbtn").on('click',()=>{
        $("#up_file").click();

    })
    $("#mainbtn1").on('click',()=>{
    })
})

    
    </script>
       <div class="main">

            
        <p id="corner">File Space</p>
        <p onclick="location.href='/logout'" id="logout">Log Out</p>
        <div class="about">
        
            <p>Aakash kumar</p>
        </div>
        <div class="panel">
            <p onclick="location.href='/panel'">Control Panel</p>
        </div>
        </div>

        <div class="display">
            <% if(locals.users) {%>
            <% for(let i=0;i<users.length;i++){ %>
            <button class="mainbtn" onclick="location.href='/users/<%=users[i].email %>'"><%= users[i].email %></button>
            <% } %>
            <% }else{ %>
                <p>No one sharing their Space with you</p>
            <% } %>

        </div>
      
   
    </body>
    </html>